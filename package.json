{
  "$schema": "https://enconvo.com/schemas/extension.json",
  "version": "0.0.10",
  "author": "ysnows",
  "license": "MIT",
  "categories": [
    "File"
  ],
  "type": "module",
  "name": "calender",
  "title": "Apple Calendar",
  "description": "Manage your schedule with Apple Calendar.",
  "icon": "calender.png",
  "minAppVersion": "1.4.9",
  "commands": [
    {
      "name": "add_event_to_apple_calender",
      "title": "Add To Apple Calendar",
      "description": "Add an event to Apple Calendar.",
      "icon": "calender.png",
      "mode": "no-view",
      "parameters": {
        "type": "object",
        "properties": {
          "input_text": {
            "type": "string",
            "description": "The natural language description of the event"
          },
          "title": {
            "type": "string",
            "description": "The title of the calendar event"
          },
          "startDate": {
            "type": "string",
            "description": "Start date and time in format 'yyyy-MM-dd HH:mm:ss'"
          },
          "endDate": {
            "type": "string",
            "description": "End date and time in format 'yyyy-MM-dd HH:mm:ss'"
          },
          "notes": {
            "type": "string",
            "description": "Additional notes or description for the event"
          },
          "isAllDay": {
            "type": "boolean",
            "description": "Whether this is an all-day event",
            "default": false
          },
          "url": {
            "type": "string",
            "description": "URL associated with the event"
          },
          "location": {
            "type": "string",
            "description": "Location of the event"
          },
          "calendarId": {
            "type": "string",
            "description": "ID of calendar to add event to, uses default if not specified"
          },
          "availability": {
            "type": "integer",
            "description": "Event availability status (0: free, 1: busy, etc)"
          },
          "alarms": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "description": "Array of alarm offset times in seconds before event (negative values, e.g. -3600 for 1 hour before)"
          },
          "recurrence": {
            "type": "object",
            "properties": {
              "frequency": {
                "type": "string",
                "enum": [
                  "daily",
                  "weekly",
                  "monthly",
                  "yearly"
                ],
                "description": "Frequency of event recurrence"
              },
              "interval": {
                "type": "integer",
                "description": "Repeat every X days/weeks/months/years"
              },
              "endDate": {
                "type": "string",
                "description": "End date for recurrence in format 'yyyy-MM-dd HH:mm:ss'"
              },
              "count": {
                "type": "integer",
                "description": "Number of occurrences"
              }
            },
            "description": "Recurrence rule for repeating events"
          }
        },
        "required": [
          "title",
          "startDate",
          "endDate"
        ]
      },
      "preferences": [
        {
          "name": "llm",
          "description": "The model used to generate responses",
          "type": "extension",
          "required": false,
          "default": "default",
          "title": "LLM Provider"
        }
      ]
    },
    {
      "name": "update_calendar_event",
      "title": "Update Calendar Event",
      "description": "Update an event to Apple Calendar.",
      "icon": "calender.png",
      "mode": "no-view",
      "parameters": {
        "type": "object",
        "properties": {
          "input_text": {
            "type": "string",
            "description": "The natural language description of the event"
          },
          "eventId": {
            "type": "string",
            "description": "The ID of the event to update"
          },
          "title": {
            "type": "string",
            "description": "The title of the calendar event"
          },
          "startDate": {
            "type": "string",
            "description": "Start date and time in format 'yyyy-MM-dd HH:mm:ss'"
          },
          "endDate": {
            "type": "string",
            "description": "End date and time in format 'yyyy-MM-dd HH:mm:ss'"
          },
          "notes": {
            "type": "string",
            "description": "Additional notes or description for the event"
          },
          "isAllDay": {
            "type": "boolean",
            "description": "Whether this is an all-day event",
            "default": false
          },
          "url": {
            "type": "string",
            "description": "URL associated with the event"
          },
          "location": {
            "type": "string",
            "description": "Location of the event"
          },
          "calendarId": {
            "type": "string",
            "description": "ID of calendar to add event to, uses default if not specified"
          },
          "availability": {
            "type": "integer",
            "description": "Event availability status (0: free, 1: busy, etc)"
          },
          "alarms": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "description": "Array of alarm offset times in seconds before event (negative values, e.g. -3600 for 1 hour before)"
          },
          "recurrence": {
            "type": "object",
            "properties": {
              "frequency": {
                "type": "string",
                "enum": [
                  "daily",
                  "weekly",
                  "monthly",
                  "yearly"
                ],
                "description": "Frequency of event recurrence"
              },
              "interval": {
                "type": "integer",
                "description": "Repeat every X days/weeks/months/years"
              },
              "endDate": {
                "type": "string",
                "description": "End date for recurrence in format 'yyyy-MM-dd HH:mm:ss'"
              },
              "count": {
                "type": "integer",
                "description": "Number of occurrences"
              }
            },
            "description": "Recurrence rule for repeating events"
          }
        },
        "required": [
          "eventId",
          "title",
          "startDate",
          "endDate"
        ]
      },
      "preferences": [
        {
          "name": "llm",
          "description": "The model used to generate responses",
          "type": "extension",
          "required": false,
          "default": "default",
          "title": "LLM Provider"
        }
      ]
    },
    {
      "name": "get_calendar_events",
      "title": "Get Calendar Events",
      "description": "Get calendar events.",
      "icon": "calender.png",
      "mode": "no-view",
      "parameters": {
        "type": "object",
        "properties": {
          "days": {
            "type": "number",
            "description": "The number of days to get events for"
          }
        }
      }
    },
    {
      "name": "delete_calendar_event",
      "title": "Delete Calendar Event",
      "description": "Delete calendar event.",
      "icon": "calender.png",
      "mode": "no-view",
      "parameters": {
        "type": "object",
        "properties": {
          "eventId": {
            "type": "string",
            "description": "The ID of the event to delete"
          },
          "deleteAllRecurrences": {
            "type": "boolean",
            "description": "Whether to delete all recurrence events, default is false"
          }
        }
      }
    },
    {
      "name": "get_calendar_list",
      "title": "Get Calendar List",
      "description": "Get calendar list.",
      "icon": "calender.png",
      "mode": "no-view",
      "parameters": {
        "type": "object",
        "properties": {}
      }
    }
  ],
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/react-reconciler": "^0.28.9",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
